<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Organization",
  "url": "<%= ss('general.url') %>",
  "logo": "<%= ss('theme.branding.logo') %>",
  "name": "<%= ss('general.name')%>",
  "legalName": "<%= ss('general.legal_name')%>",
  "contactPoint": [
      { "@type": "ContactPoint",
        "telephone": "<%= ss('contact.phone') %>",
        "contactType": "customer service",
        "availableLanguage": "English"
      }
    ],
  "sameAs": [
     "https://twitter.com/<%= ss('contact.twitter').gsub("@","") %>"
  ]
}
</script>